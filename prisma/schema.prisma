// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Country {
  id        Int      @id @default(autoincrement())
  name      String
  languages String[]
  cca2      String
  cca3      String
  ccn3      Int
  currencies Json 
  region    String
  latlng Decimal[]
}

